<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/style.css" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css"
      integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T"
      crossorigin="anonymous"
    />
<link rel="icon" href="/images/garbanzo-favicon.png" type="image">
    <title>Garbanzo AI</title>
  </head>
  <body>
    <header>
      <div class = "head">
      
   <a href="/">   <img class="logo" src="/images/GarbanzoAI-logo.png" alt="logo"  /> </a>
      
        <div>
        {{#if logged_in}}
          <a class="head-btn profile up-btn" href="/home">New image</a>
          <a class="head-btn profile up-btn" id="profile" href="/dashboard">Dashboard</a>
          
          <button class="head-btn up-btn" id="logout">Logout</button>
        {{else}}
          <div>
            <a class="login head-btn" href="/login">Login</a>
          </div>
        {{/if}}
        </div>
      </div>
    </header>
    <main>
      <div>
   
        {{{body}}}</div>

    </main>

    

    <!-- Render script for logged in users only -->
    {{#if logged_in}}
      <script src="/js/logout.js"></script>
    {{/if}}
      {{!-- <script src="/js/dashboard.js"></script> --}}
  </body>
</html>